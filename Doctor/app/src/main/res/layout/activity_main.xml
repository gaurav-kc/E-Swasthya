<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rgn.doctor.MainActivity">
    <com.roughike.bottombar.BottomBar
        android:id="@+id/bottomBar"
        android:layout_width="wrap_content"
        android:layout_height="1127dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        app:bb_tabXmlResource="@xml/bottombar_tabs_color_changing"
        app:bb_tabletMode="true"
        app:bb_inActiveTabColor="#FFFFFF"
        app:bb_activeTabColor="#FFFFFF"
        app:bb_inActiveTabAlpha="0.6"
        app:bb_activeTabAlpha="1"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="0dp" />
<FrameLayout
    android:layout_width="784dp"
    android:layout_height="1127dp"
    android:id="@+id/framelayout"
    tools:layout_editor_absoluteY="8dp"
    tools:layout_editor_absoluteX="8dp">
</FrameLayout>
</android.support.constraint.ConstraintLayout>
